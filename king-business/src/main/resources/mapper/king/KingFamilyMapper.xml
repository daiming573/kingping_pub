<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.king.pig.dao.KingFamilyDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.king.pig.entity.KingFamily">
        <id column="id" property="id" />
        <result column="family_name" property="familyName" />
        <result column="family_account" property="familyAccount" />
        <result column="status" property="status" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, family_name AS familyName, family_account AS familyAccount, status, create_time AS createTime, update_time AS updateTime
    </sql>

</mapper>
